{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 15, "lineWidth": 2, "spanNulls": false },
          "unit": "reqps"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "sum(rate(nextjs_http_requests_total[1m])) by (route)",
          "legendFormat": "{{route}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Request Duration P95 (seconds)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        },
        "overrides": []
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nextjs_http_request_duration_seconds_bucket[5m])) by (le, route))",
          "legendFormat": "p95 {{route}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Total Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "blue", "value": null }] }
        }
      },
      "options": { "colorMode": "background", "textMode": "value_and_name" },
      "targets": [
        {
          "expr": "sum(nextjs_http_requests_total)",
          "legendFormat": "Total Requests",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "red", "value": 5 }] },
          "unit": "percent"
        }
      },
      "options": { "colorMode": "background" },
      "targets": [
        {
          "expr": "sum(rate(nextjs_http_request_errors_total[5m])) / sum(rate(nextjs_http_requests_total[5m])) * 100",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ]
    },
    {
      "title": "In-Flight Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 5 }, { "color": "red", "value": 10 }] }
        }
      },
      "options": { "colorMode": "background" },
      "targets": [
        {
          "expr": "sum(nextjs_http_requests_in_flight)",
          "legendFormat": "In-Flight",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Avg Response Time",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 1 }, { "color": "red", "value": 5 }] },
          "unit": "s"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(nextjs_http_request_duration_seconds_sum[5m])) / sum(rate(nextjs_http_request_duration_seconds_count[5m]))",
          "legendFormat": "Avg Latency",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Enrolled Students",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "purple", "value": null }] }
        }
      },
      "targets": [
        {
          "expr": "nextjs_enrolled_students_total",
          "legendFormat": "Students",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Seeded Descriptors",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "steps": [{ "color": "purple", "value": null }] }
        }
      },
      "targets": [
        {
          "expr": "nextjs_seeded_descriptors_total",
          "legendFormat": "Seeded",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Requests by Route & Status",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          { "matcher": { "id": "byName", "options": "Value" }, "properties": [{ "id": "displayName", "value": "Requests" }] }
        ]
      },
      "targets": [
        {
          "expr": "sum by (route, method, status_code) (nextjs_http_requests_total)",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "transformations": [
        { "id": "organize", "options": { "excludeByName": { "Time": true }, "renameByName": { "method": "Method", "route": "Route", "status_code": "Status" } } }
      ]
    },
    {
      "title": "External Service Duration (Firebase, Hikvision, BINUS API)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10, "lineWidth": 2 },
          "unit": "s"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nextjs_external_call_duration_seconds_bucket[5m])) by (le, service, operation))",
          "legendFormat": "{{service}} / {{operation}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Rate by Route",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "bars", "fillOpacity": 50, "lineWidth": 1 },
          "unit": "reqps"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(nextjs_http_request_errors_total[5m])) by (route)",
          "legendFormat": "{{route}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Request/Response Size",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 10 },
          "unit": "bytes"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(nextjs_http_request_size_bytes_bucket[5m])) by (le))",
          "legendFormat": "Request p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(nextjs_http_response_size_bytes_bucket[5m])) by (le))",
          "legendFormat": "Response p95",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Node.js Process — Memory (RSS)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "orange", "mode": "fixed" },
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2 },
          "unit": "bytes"
        }
      },
      "targets": [
        {
          "expr": "nextjs_process_resident_memory_bytes",
          "legendFormat": "RSS",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Node.js Process — CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "green", "mode": "fixed" },
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2 },
          "unit": "percentunit"
        }
      },
      "targets": [
        {
          "expr": "rate(nextjs_process_cpu_seconds_total[5m])",
          "legendFormat": "CPU %",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Node.js Event Loop Lag",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "color": { "fixedColor": "yellow", "mode": "fixed" },
          "custom": { "drawStyle": "line", "fillOpacity": 20, "lineWidth": 2 },
          "unit": "s"
        }
      },
      "targets": [
        {
          "expr": "nextjs_nodejs_eventloop_lag_seconds",
          "legendFormat": "Event Loop Lag",
          "refId": "A"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["binus", "facial-attendance", "api"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "Asia/Jakarta",
  "title": "BINUS Facial Attendance — API Performance",
  "uid": "binus-facial-attendance",
  "version": 1
}
